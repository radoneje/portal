extends layout

block content
  style.
    body{background: #000;color:white}
  div(class="main-page" id="main-page" v-bind:class="{loaded:isLoaded}")
    include elements/head
    include elements/right
    include elements/left
    div(class="main-page-container")
      div(class="main-page-r")
    div.loadingCtrl(id="loadingCtrl" style="position:fixed; bottom:0;left:0;width: 100vw;height: 2px;")
  script var clientId=#{clientId};
  script(src="/javascripts/groupscript.js")
